{
	"cmake.configureOnOpen": false,
	"C_Cpp.default.includePath":
			["${default}",
			 "${workspaceFolder}/ccycles",
			 "${workspaceFolder}/glew/include",
			 "${workspaceFolder}/cycles/src/**",
			 "${workspaceFolder}/cycles/third_party/**",
			 "${workspaceFolder}/../../../../../big_libs/OpenImageIO-2.2.19/include",
			 "${workspaceFolder}/../../../../../big_libs/boost",
			 "${workspaceFolder}/../../../../../big_libs/OpenEXR-2.5.5/include",
			 "${workspaceFolder}/**"
			],
	"C_Cpp.default.defines" : [
			"CCL_NAMESPACE_BEGIN=namespace ccl {",
			"CCL_NAMESPACE_END=}",
			"WITH_CUDA_DYNLOAD",
			"__CUDA_ARCH__",
			"WITH_OPENCL"],
	"files.associations": {
		"*.u3m": "json",
		"vector": "cpp",
		"fstream": "cpp",
		"istream": "cpp",
		"streambuf": "cpp",
		"condition_variable": "cpp",
		"cstdlib": "cpp",
		"future": "cpp",
		"mutex": "cpp"
	}
}